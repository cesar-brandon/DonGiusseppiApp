<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.tabs.StartMenuFragment">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nuestra"
        android:textSize="20sp"
        android:textColor="@color/md_theme_primary"
        android:translationX="40dp"
        android:translationY="40dp" />
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text="Carta"
        android:textColor="@color/complement"
        android:translationX="125dp"
        android:translationY="40dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pizzas"
        android:translationX="40dp"
        android:translationY="90dp"/>
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Helados"
        android:translationX="40dp"
        android:translationY="250dp"/>
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Postres"
        android:translationX="40dp"
        android:translationY="420dp"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="283dp"
        android:layout_height="169dp"
        android:src="@drawable/menu_pizza"
        android:translationX="40dp"
        android:translationY="90dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="282dp"
        android:layout_height="184dp"
        android:src="@drawable/menu_helado"
        android:translationX="40dp"
        android:translationY="250dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="283dp"
        android:layout_height="190dp"
        android:src="@drawable/menu_postre"
        android:translationX="40dp"
        android:translationY="420dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Y mas..."
        android:translationX="40dp"
        android:translationY="600dp"/>

</FrameLayout>